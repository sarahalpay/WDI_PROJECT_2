<div class="container">
<h1>MY LIKES</h1>
<div class="row">
<% user.favorites.forEach((brand) => { %>
  <div class="col-sm-12 col-md-6 col-lg-3">
  <a href="#">
  <h5><%= brand.name %></h5>
  <img class="img-thumbnail" src="<%= brand.mainimage %>" alt="<%= brand.name %>">
  </a>
  </div>
<% }); %>
</div>
</div>
